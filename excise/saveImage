from PIL import Image
import os, glob
filelist=glob.glob("d:\filelist\*.jpg")

for infile in filelist:
    outfile = os.path.splitext(infile)[0]+".jpg"
    if infile !=outfile:
        try:
            Image.open(infile).save(outfile)
        except IOError:
            print("cannot convert", infile)
            
